	// function to determine highlighted text and black it out
	function blackoutHighlightedText() {
		if(highlightedText)
		{
			blackOutText(highlightedText);
			highlightedText = null;
		}
	}

	// function to determine highlighted	
	function getHighlightedText() {
		var range = window.getSelection().getRangeAt(0);
		console.log(range);
		highlightedText = range.toString();
	}
	
	function clearHighlightedText() {
		highlightedText = null;
	}
	
	function blackOutText(message){
		var originalText = $('#htmlresume').html();
		var rangeStart = originalText.indexOf(message);
		var rangeLength = message.length;
		
		var blankString = "";
		for(var i = 0; i < rangeLength; i++){
			blankString += "n";
		}
		blackoutText = "<span style='background-color:#333;color:#333;text-decoration:none;'>"+blankString+"</span>";

		// have to add 6 for a <span> that shows up randomly between start and now
		var newText = originalText.substring(0,rangeStart) + blackoutText + originalText.substring(rangeStart+rangeLength, originalText.length);
		
		$('#htmlresume').html(newText);
	}
	
	function clearBlackoutDivs() {
		var priorDIVs = $('[id$=hideButton]');
		priorDIVs.each(function(){
			$(this).remove();
		});
	}
	
	function showBlackoutButton(yPosition) {
	  var xPosition = $('#htmlresume').position().left - 75;
		clearBlackoutDivs();
		var buttonDiv = $("<div id='hideButton'>");
		buttonDiv.addClass('hide_data_bubble');
		buttonDiv.css({"top":yPosition-12,"left":xPosition});
		buttonDiv.append($("<div style='float:left;padding:2px;border-right:1px solid #1E608F;'>X</div>"));
		buttonDiv.append($("<div style='float:left;padding-top:2px;padding-left:8px;padding-right:8px;'>Hide</div>"));
		buttonDiv.append($("<div style='clear:both;'></div>"));
		buttonDiv.click(function(){blackoutHighlightedText();$(this).remove();});
		$("body").append(buttonDiv);
	}
	
